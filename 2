package assignment;
import java.util.*;
public class Assignment2 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Enter Two float numbers");
		float a,b,ans;
		a = in.nextFloat();
		b = in.nextFloat();
		
		ans = (a*b);
		System.out.println("Multiply of given float number is " +ans);

	}

}
